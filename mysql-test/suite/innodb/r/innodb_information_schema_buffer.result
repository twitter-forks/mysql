SELECT SLEEP(1);
SLEEP(1)
0
SELECT 1 AS STATUS
FROM stats, INFORMATION_SCHEMA.INNODB_BUFFER_POOL_STATS AS bp
WHERE stats.NUMBER_PAGES_CREATED <= bp.NUMBER_PAGES_CREATED;
STATUS
1
SELECT 1 AS STATUS
FROM stats, INFORMATION_SCHEMA.INNODB_BUFFER_POOL_STATS AS bp
WHERE stats.NUMBER_PAGES_WRITTEN <= bp.NUMBER_PAGES_WRITTEN;
STATUS
